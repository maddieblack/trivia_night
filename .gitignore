# Ignore bundler config.
/api/.bundle

# Ignore the default SQLite database.
/api/db/*.sqlite3
/api/db/*.sqlite3-journal
/api/db/*.sqlite3-*

# Ignore all logfiles and tempfiles.
/api/log/*
/api/tmp/*
!/api/log/.keep
!/api/tmp/.keep


